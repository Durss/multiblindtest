{
  "name": "multiblindtest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run front/serve\" \"npm run server/watch\"",
    "build": "npm run front/build && npm run server/build",
    "front/serve": "vue-cli-service serve",
    "front/build": "vue-cli-service build",
    "server/watch": "tsc -w -p ./tsconfig_server.json",
    "server/build": "tsc -p ./tsconfig_server.json",
    "server/start": "pm2 start bootstrap-pm2.json"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.8",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "express-serve-static-core": "^0.1.1",
    "glob": "^7.1.6",
    "http": "0.0.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "uuid": "^8.3.2",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@types/sockjs": "^0.3.32",
    "@types/tmi.js": "^1.7.1",
    "@types/universal-analytics": "^0.4.4",
    "@vue/cli-plugin-router": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-plugin-vuex": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "concurrently": "^5.3.0",
    "fuse.js": "^6.4.6",
    "gsap": "^3.6.0",
    "html-webpack-plugin": "^4.5.1",
    "less": "^4.1.0",
    "less-loader": "^7.2.1",
    "tmi.js": "^1.7.1",
    "typescript": "~4.1.3",
    "universal-analytics": "^0.4.23",
    "vue-i18n": "^8.22.3",
    "vue-template-compiler": "^2.6.12"
  }
}
